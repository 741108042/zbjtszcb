{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Employee</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ entity.age }}</td>
            </tr>
            <tr>
                <th>Sex</th>
                <td>{{ entity.sex }}</td>
            </tr>
            <tr>
                <th>Education</th>
                <td>{{ entity.education }}</td>
            </tr>
            <tr>
                <th>Createtime</th>
                <td>{{ entity.createtime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatetime</th>
                <td>{{ entity.updatetime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Photopath</th>
                <td>{{ entity.photopath }}</td>
            </tr>
            <tr>
                <th>Jlpath</th>
                <td>{{ entity.jlpath }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('employee') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('employee_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
